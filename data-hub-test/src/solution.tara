Transaction  pagos
	Attribute a as LongInteger(size = 48bits)
	Attribute b as Integer
	Attribute c as DateTime
	Attribute d as Bool
	Attribute giro as WordBag(wordBag = Giro)
	Attribute sector as WordBag(wordBag = Sector)
	Attribute gestion as WordBag(wordBag = Gestion)
	Attribute municipio as WordBag(wordBag = Municipio)

WordBag Giro
	Word(1) Federal
	Word(2) Municipal
	Word(3) Estatal
	Word(4) ParaEstatal
	Word(5) Publico
WordBag Sector
	Word(1) Comercial
	Word(2) Agricola
	Word(3) Industrial
	Word(4) Alumbrado
	Word(5) Residencial
WordBag Gestion
	Word(1) Rezago
	Word(2) Convenio
	Word(3) Incobrable
	Word(4) Normal

WordBag Municipio as  FromResource(tsv = "Municipios.tsv")

Event Example
	Attribute name as Text
	Attribute names as Text List

Terminal first
	Publish(tanks = dl.example)
	Subscribe (tanks = dl.example)
