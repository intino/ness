dsl Ness

use Datamart
use Datalake

Broker(path = empty, port = 63000, secondaryPort = 1884)
	User("trooper", "trooper")
	User("wizard", "wizard")
	User("consul", "consul")
	User("connector", "connector")
	User("master", "master")

Terminal trooper-terminal
	Subscribe(dl.diagnosisAssertion dl.state, dl.analysis dl.assetStatus dl.applicationStatus dl.rdbmsStatement)
	Publish(dl.notification dl.assetAssertion dl.hardwareAssertion dl.peripheralDeviceAssertion dl.networkDeviceAssertion dl.machineAssertion dl.softwareAssertion dl.consulAssertion dl.serviceAssertion dl.rdbmsServiceAssertion dl.queryAssertion dl.applicationAssertion dl.javaApplicationAssertion dl.businessUnitAssertion)
	Datamarts(master)

Terminal wizard-terminal
	Publish(dl.diagnosisAssertion)
	Subscribe(dl.state dl.machineAssertion dl.diagnosisAssertion, dl.analysis dl.assetStatus dl.rdbmsStatement dl.applicationStatus)
	Datamarts(master)

Terminal connector-terminal
	Publish(dl.userAssertion dl.channelAssertion)
	Subscribe(dl.notification)
	Datamarts(master)

Terminal master-terminal
	Publish(dl.diagnosisAssertion dl.state dl.areaAssertion dl.userAssertion dl.channelAssertion dl.teamAssertion dl.assetAssertion dl.hardwareAssertion dl.peripheralDeviceAssertion dl.networkDeviceAssertion dl.machineAssertion dl.softwareAssertion dl.consulAssertion dl.serviceAssertion dl.rdbmsServiceAssertion dl.queryAssertion dl.applicationAssertion dl.javaApplicationAssertion dl.businessUnitAssertion, dl.assetStatus dl.applicationStatus dl.rdbmsStatement)
	Subscribe(dl.diagnosisAssertion  dl.state, empty)
	Datamarts(master)

Terminal test-terminal
	Subscribe(dl.diagnosisAssertion dl.state, dl.analysis dl.assetStatus dl.applicationStatus dl.rdbmsStatement)
	Publish(dl.notification dl.assetAssertion dl.hardwareAssertion dl.peripheralDeviceAssertion dl.networkDeviceAssertion dl.machineAssertion dl.softwareAssertion dl.consulAssertion dl.serviceAssertion dl.rdbmsServiceAssertion dl.queryAssertion dl.applicationAssertion dl.javaApplicationAssertion dl.businessUnitAssertion, dl.assetStatus)
	Datamarts(master)
