dsl Ness

use Datamart
use Datalake

Broker(path = empty, port = 63000, secondaryPort = 1884)
	User("trooper", "trooper")
	User("monet", "monet")
	User("wizard", "wizard")
	User("consul", "consul")
	User("cesar", "cesar")
	User("connector", "connector")
	User("master", "master")

Terminal monet-terminal
	Subscribe(dl.assertion dl.push dl.pop)
	Publish(dl.assertion dl.push dl.pop)
	Datamarts(master)

Terminal observer-terminal
	Publish(dl.status dl.incident dl.placeAssertion dl.areaAssertion dl.modelAssertion dl.orderTypeAssertion dl.personAssertion dl.processAssertion dl.assetAssertion dl.partAssertion dl.hardwareAssertion dl.computerAssertion dl.applianceAssertion dl.softwareAssertion dl.observerAssertion dl.applicationAssertion dl.applicationJavaAssertion dl.serviceAssertion, dl.computer dl.application dl.camera dl.cellularGateway dl.switch dl.wireless)
	Datamarts(master, autoLoad = false)

Terminal trooper-terminal
	Subscribe(dl.diagnosisAssertion dl.status dl.incident dl.rollout dl.assertion dl.push dl.pop dl.inform dl.staffLogin dl.staffLogout dl.workOrderAssertion dl.staffWorkReport dl.staffEmailSubscription dl.staffObservableVisit dl.notification, dl.analysis dl.computer dl.application dl.camera dl.cellularGateway dl.switch dl.rdbmsStatement)
	Publish(dl.partAssertion dl.assetAssertion dl.hardwareAssertion dl.applianceAssertion dl.observerAssertion dl.computerAssertion dl.softwareAssertion dl.serviceAssertion dl.applicationAssertion dl.applicationJavaAssertion dl.processAssertion dl.personAssertion dl.rollout dl.assertion dl.push dl.pop dl.inform dl.staffLogin dl.staffLogout dl.workOrderAssertion dl.staffWorkReport dl.staffEmailSubscription dl.staffObservableVisit dl.notification dl.activityInstalled dl.activityUninstalled dl.activityConfiguration dl.activityUpgraded dl.activityEnabled dl.activityDisabled dl.operationExecuted)
	Datamarts(master, autoLoad = false)

Terminal wizard-terminal
	Publish(dl.diagnosisAssertion dl.incident)
	Subscribe(dl.status dl.computerAssertion dl.diagnosisAssertion, dl.analysis dl.computer dl.rdbmsStatement dl.application dl.camera dl.cellularGateway dl.switch)
	Datamarts(master)

Terminal master-terminal
	Publish(dl.diagnosisAssertion dl.status dl.areaAssertion dl.partAssertion dl.assetAssertion dl.hardwareAssertion dl.applianceAssertion dl.computerAssertion dl.softwareAssertion dl.observerAssertion dl.serviceAssertion dl.processAssertion dl.personAssertion dl.applicationAssertion dl.applicationJavaAssertion, dl.computer dl.application dl.rdbmsStatement)
	Subscribe(dl.diagnosisAssertion dl.status, empty)
	Datamarts(master)